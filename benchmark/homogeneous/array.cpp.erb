#include <array>
#include <cstdlib>


int main () {
    unsigned long long result = 0;
    for (int iteration = 0; iteration < 1 << 10; ++iteration) {

        std::array<int, <%= n %>> ints{{
            <%= (1..n).map { 'std::rand()' }.join(', ') %>
        }};
        int sum = 0;
        for (int i : ints)
            sum += i;

        result += sum;
    }
}
