template <int>
struct arg { using type = arg; };

template <typename ...>
struct function { };

template <>
struct function<
    <%= (1..n).map { |i| "arg<#{i}>::type" }.join(', ') %>
> { };

int main() {
    function<
        <%= (1..n).map { |i| "arg<#{i}>" }.join(', ') %>
    >{};
}
